{
  "name": "whiteboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mf": "cross-env ENABLE_MF=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:federation": "ENABLE_MF=true vite --open /test-federation.html",
    "deploy:dev": "chmod +x deploy.sh && ./deploy.sh dev",
    "deploy:prod": "chmod +x deploy.sh && ./deploy.sh prod",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod",
    "sync:dev": "npm run build && serverless s3sync --stage dev",
    "sync:prod": "npm run build && serverless s3sync --stage prod",
    "invalidate:dev": "serverless cloudfrontInvalidate --stage dev",
    "invalidate:prod": "serverless cloudfrontInvalidate --stage prod",
    "mf-dev": "ENABLE_MF=true vite",
    "mf-build": "vite build",
    "mf-test": "npm run mf-build && python3 -m http.server 8080",
    "start-mf": "npm run build && npm run preview",
    "test-federation": "echo 'Abriendo test de Module Federation...' && open test-microfrontend-final.html || xdg-open test-microfrontend-final.html"
  },
  "dependencies": {
    "katex": "^0.16.22",
    "konva": "^9.3.20",
    "vue": "^3.5.17",
    "vue-konva": "^3.2.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.6",
    "@types/node": "^24.0.4",
    "@vitejs/plugin-vue": "^4.5.0",
    "@volar-plugins/pug": "^2.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "pug": "^3.0.3",
    "serverless": "^3.38.0",
    "serverless-cloudfront-invalidate": "^1.12.2",
    "serverless-s3-sync": "^3.2.0",
    "typescript": "^5.8.3",
    "vite": "^4.5.1",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
